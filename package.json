    "exceljs": "^4.4.0",
    "jspreadsheet-ce": "^5.0.2",
    "jsuites": "^5.11.0",
    "jszip": "^3.10.1",
    "libreoffice-convert": "^1.7.0",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.514.0",
    "next": "^15.5.6",
    "node-fetch": "^3.3.2",
    "react": "^19.0.0",
    "react-aria": "^3.41.1",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.2",
    "react-stately": "^3.39.0",
    "simple-odf": "^3.0.3",
    "tailwind-merge": "^3.3.1",
    "xlsx": "^0.18.5",
    "xlsx-js-style": "^1.2.0",
    "xml2js": "^0.6.2",
    "zod": "^3.25.62"
