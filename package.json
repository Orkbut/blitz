{
  "name": "radar-detran",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:fast": "next dev --turbo",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "dev:all": "concurrently \"npm run dev\" \"npx -y @supabase/mcp-server-supabase@latest --access-token sbp_3227b5e8f492eec4e20bc0427471ff4def64da3\" \"npx @playwright/mcp@latest\" \"npx -y @upstash/context7-mcp@latest\" --names \"NEXT,SUPABASE,PLAYWRIGHT,CONTEXT7\" --prefix-colors \"blue,yellow,magenta,cyan\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:config-validator": "vitest run src/tests/config-validator.test.ts",
    "test:concurrency": "npx tsx src/tests/test-concurrency.ts",
    "test:realtime": "npx tsx src/tests/test-realtime-concurrency.ts",
    "test:simple": "npx tsx src/tests/test-simple-validation.ts",
    "analyze:functions": "npx tsx analyze-functions.ts",
    "analyze:buttons": "npx tsx analyze-buttons.ts",
    "analyze:eu-vou": "npx tsx analyze-eu-vou-cancelar.ts",
    "debug:eu-vou": "npx tsx debug-eu-vou-issue.ts",
    "analyze:deep": "npm run analyze:functions && npm run analyze:buttons",
    "db:migrate": "echo 'Execute as migrations no Supabase Dashboard: src/migrations/005_add_concurrency_constraints.sql'",
    "db:migrate:rollback": "echo 'Para rollback, execute: src/migrations/005_add_concurrency_constraints_rollback.sql'",
    "pwa:setup": "node scripts/generate-pwa-icons.cjs",
    "pwa:validate": "npx pwa-asset-generator --help || echo 'Para validar PWA: npm install -g pwa-asset-generator'",
    "icons:generate": "node scripts/generate-icons.cjs",
    "build:pwa": "npm run build && npm run start"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.13",
